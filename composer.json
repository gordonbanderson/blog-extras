{
  "name": "suilven/blog-extras",
  "type": "silverstripe-vendormodule",
  "description": "Additions to the standard SilverStripe blog module.",
  "homepage": "https://github.com/gordonbanderson/blog-extras",
  "keywords": ["blog"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Gordon Anderson",
      "role": "Developer",
      "email": "gordon.b.anderson@gmail.com",
      "homepage": "https://github.com/gordonbanderson"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "silverstripe/framework": "^4@dev"
  },
  "autoload": {
    "psr-4": {
      "Suilven\\Blog\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "expose": [
      "admin/client/dist",
      "client/dist"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
